    "type": "git",
    "url": "git+https://github.com/hoangsonww/The-MovieVerse-Database.git"
  },
  "bugs": {
    "url": "https://github.com/hoangsonww/The-MovieVerse-Database/issues"
  },
  "homepage": "https://movie-verse.com",
