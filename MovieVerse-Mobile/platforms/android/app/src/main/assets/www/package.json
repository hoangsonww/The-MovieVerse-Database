{
  "name": "themovieverseapp",
  "displayName": "The MovieVerse App",
  "version": "1.0.0",
  "description": "An interactive movie database app that allows users to search for movies, view movie details, and save movies to their favorites list",
  "main": "index.js",
  "scripts": {
    "install-deps": "npm install",
    "start": "webpack-dev-server --mode development --open",
    "build": "webpack --mode production",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "eslint . --ext .js,.jsx --fix",
    "format": "prettier --write '**/*.{js,jsx,json,css,scss,html}'",
    "serve": "http-server dist -o",
    "dev:android": "cordova run android",
    "dev:ios": "cordova run ios",
    "build:android": "cordova build android",
    "build:ios": "cordova build ios",
    "eject": "react-scripts eject"
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "author": "Son Nguyen Hoang",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-react": "^7.23.3",
    "babel-loader": "^9.1.3",
    "cordova-android": "^12.0.1",
    "cordova-ios": "^7.0.1",
    "electron": "^28.2.1",
    "electron-builder": "^24.9.1",
    "eslint": "^8.56.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-react": "^7.33.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "cordova": {
    "platforms": [
      "android",
      "ios"
    ]
  },
  "dependencies": {
    "@testing-library/dom": "^9.3.4",
    "axios": "^1.6.5",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "django": "^1.0.0",
    "docker": "^1.0.0",
    "dotenv": "^16.3.1",
    "expect": "^29.7.0",
    "express": "^4.18.2",
    "i18next": "^23.7.11",
    "jsonwebtoken": "^9.0.2",
    "kubernetes": "^1.0.0",
    "lodash": "^4.17.21",
    "mongoose": "^8.0.3",
    "node": "^21.2.0",
    "prettier": "^3.1.1",
    "ramda": "^0.28.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^7.2.9",
    "react-router-dom": "^6.21.2",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1",
    "single-spa": "^6.0.0",
    "styled-components": "^5.3.5",
    "vue": "^3.3.13",
    "vue-router": "^4.0.12",
    "vuex": "^4.0.2",
    "webpack-dev-server": "^4.7.3"
  }
}
